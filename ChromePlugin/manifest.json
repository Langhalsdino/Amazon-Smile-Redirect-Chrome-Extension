{
    "name": "Amazon.* smile redirect plugin",
    "description": "This plugin suggest to use smile.amazon.* insted of amazon.*",
    "author": "Langhalsdino",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html",
    "web_accessible_resources": [
        "contentNavBar.css",
        "contentNavBar.html",
        "contentNavBar.js",
        "options.js",
        "options.css"
    ],
    "permissions": [
        "<all_urls>",
        "tabs",
        "webRequest",
        "webRequestBlocking"
    ],
    "icons": {
        "128": "icon/icon128.png"
    }
}
